{
  "manifest_version": 2,
  "name": "Browser Product",
  "version": "1.0",
  "description": "Busca productos en google",
  "permissions": [
    "https://www.google.com/"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
